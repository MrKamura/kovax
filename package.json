{
  "name": "kovax",
  "version": "0.0.2",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./components/Button": {
      "types": "./dist/components/Button/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src --format esm,cjs --dts --out-dir dist --sourcemap",
    "dev": "tsup src --format esm,cjs --dts --out-dir dist --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "react",
    "ui",
    "library",
    "components"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrKamura/kovax-ui.git"
  },
  "peerDependencies": {
    "react": ">=16.0.0 <20.0.0",
    "react-dom": ">=16.0.0 <20.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
